<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.codeburrow.coordinatedmotiondemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".MultipleElements"
            android:label="@string/multiple_elements">
        </activity>
        <activity
            android:name=".MultipleChaoticElements"
            android:label="@string/multiple_chaotic_elements">
        </activity>
        <activity android:name=".CurvedMotionList">
        </activity>
        <activity android:name=".CurvedMotionDetail">
        </activity>
    </application>

</manifest>